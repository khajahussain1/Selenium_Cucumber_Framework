<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Spicejet" parallel="tests">

	<listeners>
		<listener class-name="com.cucumber.utility.Listener" />
	</listeners>
	
	<test name="BookTick">
		<classes>
			<class name="com.cucumber.TestRunner.BookTicketRunner" />
		</classes>
	</test>

	<test name="Registration">
		<classes>
			<class name="com.cucumber.TestRunner.RegistrationRunner" />
		</classes>
	</test>

	<test name="LoginRunner">
		<classes>
			<class name="com.cucumber.TestRunner.LoginRunner" />
		</classes>
	</test>

</suite>